#!/bin/sh
# $Id: nuke-desktop-gui-cruft,v 1.12 2016/10/12 00:35:20 friedman Exp $

procs='
  /^gvfsd/
  /^gvfs-/
  /gnome-.*-daemon/
  gnome-screensaver
  dconf-service
  gconfd-2
  menu-cached
  notification-daemon
  idn
  aspell
  at-spi-bus-launcher
  at-spi2-registryd
  evolution-addressbook-factory
  goa-daemon
  goa-identity-service
  libsocialweb-core
  mission-control-5
  telepathy-logger
'

extra='
  npviewer.bin
  mozplugger-helper
  plugin-container
  gpg-agent
'

case $1 in
  -f ) procs="$procs $extra"; shift ;;
esac

exec mkill $procs

# eof
