#!/bin/sh

procs='
  /^gvfs-/
  /^gvfsd/
  /gnome-.*-daemon/
  /ibus/
  aspell
  at-spi-bus-launcher
  at-spi2-registryd
  dconf-service
  evolution-addressbook-factory
  gconfd-2
  glib-pacrunner
  gnome-screensaver
  goa-daemon
  goa-identity-service
  idn
  libsocialweb-core
  menu-cached
  mission-control-5
  obexd
  notification-daemon
  telepathy-logger
  xdg-desktop-portal
  xdg-document-portal
  xdg-permission-store
'

extra='
  npviewer.bin
  mozplugger-helper
  plugin-container
  gpg-agent
'

case $1 in
  -f ) procs="$procs $extra"; shift ;;
esac

exec mkill $procs

# eof
